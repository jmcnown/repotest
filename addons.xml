<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="plugin.audio.radionl" name="Radio NL" version="1.0.6" provider-name="Beerkeeper">
    <requires>
        <import addon="xbmc.python" version="2.1.0"/>
        <import addon="script.module.urlresolver"  version="0.0.1"/>
        <import addon="script.module.metahandler"  version="1.0.2"/>
    </requires>
    <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>audio</provides>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary lang="en">Alle radiozenders van Nederland</summary>
        <description lang="en">Tot dusver bekend alle radiozenders die in Nederland worden uitgezonden</description>
        <disclaimer lang="en"></disclaimer>
        <language></language>
        <platform>all</platform>
        <license></license>
        <forum></forum>
        <website>http://www.google.nl</website>
        <email></email>
        <source></source>
    </extension>
</addon>

<addon id="script.navi-x"
       name="Navi-X"
       version="4.0"
       provider-name="Team Navi-X and TVADDONS.ag">
  <requires>
    <import addon="xbmc.python" version="2.1.0" />
    <import addon="script.module.urlresolver"  />
    <import addon="script.module.t0mm0.common"  />
    <import addon="script.module.addon.common" optional="false" />
    <import addon="repository.xbmchub" optional="true" />
    <import addon="plugin.video.youtube" optional="false" />
  </requires>
  <extension point="xbmc.python.script" library="default.py" >
    <provides>executable video</provides>
  </extension>
  <extension point="xbmc.service" library="service.py" start="startup">
  </extension>
  <extension point="xbmc.addon.metadata">
    <platform>all</platform>
    <minversion>28764</minversion>
    <summary>Media Browser Addon for Kodi</summary>
    <description>Navi-X is the oldest and most trusted add-on for Kodi, used by millions of people worldwide. You'll find everything from live television to your favourite movies and television shows, easily accessible.</description>
    <disclaimer>http://www.navixtreme.com/terms/</disclaimer>
    <forum>http://forums.tvaddons.ag/forums/38-NAVI-X-MEDIA-BROWSER-FOR-KODI</forum>
    <website>http://www.navixtreme.com/</website>
    <source></source>
    <email></email>
  </extension>
</addon>

<addon id="repository.MNS" name="MNS Repository" version="2.0" provider-name="McNown Networks">
<extension point="xbmc.addon.repository" name="MNS Addon Repository">
<info compressed="false">https://raw.github.com/jmcnown/repotest/addons.xml</info>
<checksum>https://raw.github.com/jmcnown/repotest/addons.xml.md5</checksum>
<datadir zip="true">https://raw.github.com/jmcnown/repotest/</datadir>
</extension>
<extension point="xbmc.addon.metadata">
<summary>MNS Repository</summary>
<description>McNown Netowrks Repository http://McNown.biz</description>
<platform>all</platform>
</extension>
</addon>
</addons>
